pipeline {
    agent any
	
    stages {
        stage('Executar playbook Ansible') {
            steps {
		withCredentials([string(credentialsId: 'ssh_passkey', variable: 'SSH_PASSKEY')]) {
                ansiblePlaybook(
                    inventory:'ansible/hosts',
                    playbook:'ansible/installweb.yml',
			extraVars: [
				ssh_passphrase: "${env.SSH_PASSKEY}"
			]
                )
            }
        }
    }
}
}
