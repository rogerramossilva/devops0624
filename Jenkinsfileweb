pipeline {
    agent any
	

    environment {
	SSH_PASSKEY = credentials('ssh_passkey')
    }

    stages {
        stage('Executar playbook Ansible') {
            steps {
                ansiblePlaybook(
                    inventory:'ansible/hosts',
                    playbook:'ansible/installweb.yml',
			extraVars: [
				ssh_passphrase: "${SSH_PASSKEY}"
			]
                )
            }
        }
    }
}
